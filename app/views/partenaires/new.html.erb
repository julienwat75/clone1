
<% provide(:title, "Ajouter un partenaire") %>


<div class="row" >

  <div   class="col-xs-0 col-md-4 col-lg-4 " > </div>


<!--  -----------------------------  Pour Mobile  --------------------------------------------------- -->


  <div   class="col-lg-12 col-lg-4 " >

<%= form_for(@partenaires) do |f| %>
  <ul>
  <% @partenaires.errors.full_messages.each do |error| %>
    <li><%= error %></li>
  <% end %>
  </ul>
  <div class="field" style="color:black;">
    <%= f.label :nom %><br>
    <%= f.text_field :nom %>
  </div>

  <br><br>


  <div class="field" style="color:black;">
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </div>

  <br><br>

  <div class="field" style="color:black;">
    <%= f.label :adresse %><br>
    <%= f.text_field :adresse %>
  </div>

  <br><br>

  <div class="field" style="color:black;">
    <%= f.label :telephone %><br>
    <%= f.text_field :telephone %>
  </div>

  <br><br>




  <%= f.submit "valider" , :class => "grosbouton" %>
  



  <% end %>

</div>



  <div   class="col-xs-0 col-md-4 col-lg-4 " > </div>


</div>